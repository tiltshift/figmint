{
  "name": "figmint_workspace",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "packageManager": "yarn@3.2.0",
  "prettier": "./config/prettier-config",
  "workspaces": [
    "config",
    "figmint",
    "example"
  ],
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "lint": "cd config && eslint '../**/*.{js,jsx,ts,tsx}'",
    "gh-pages": "gh-pages -d example/dist -m \"Update Github pages\"",
    "build": "cd figmint && node build.js",
    "dev": "cd figmint && ts-node src/index.tsx",
    "type-check": "cd figmint && tsc --noEmit",
    "build-types": "tsc src/utils/types.ts --outDir bin --declaration --emitDeclarationOnly",
    "release": "yarn build && yarn publish"
  },
  "resolutions": {
    "yoga-layout-prebuilt@1.10.0": "patch:yoga-layout-prebuilt@npm:1.10.0#.yarn/patches/yoga-layout-prebuilt-npm-1.10.0-855b15449f.patch"
  }
}
